<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <ui:VisualElement name="Root" style="flex-grow: 1; background-color: rgb(58, 58, 58);">
        <ui:DropdownField label="States" name="StatesField" style="color: rgb(255, 255, 255); -unity-text-align: upper-center; font-size: 29px;" />
        <ui:VisualElement name="StateStatus" style="flex-grow: 1; margin-top: 45px; display: flex;">
            <ui:Label tabindex="-1" text="Label" parse-escape-sequences="true" display-tooltip-when-elided="true" name="CurrentStateText" style="-unity-text-align: upper-center; font-size: 79px; color: rgb(255, 255, 255);" />
            <ui:DropdownField label="ClassName" name="BindStateField" style="margin-left: 89px; margin-right: 51px; -unity-text-align: upper-right; color: rgb(255, 255, 255); font-size: 23px;" />
            <ui:VisualElement name="Transition" style="flex-grow: 1; align-items: stretch; justify-content: flex-start; flex-direction: column; margin-bottom: 0; padding-top: 15px;">
                <ui:VisualElement name="CreateTransition" style="flex-grow: 0.1; flex-direction: row; height: 16px; flex-basis: 1%;">
                    <ui:DropdownField label="Transition" name="TransitionField" style="justify-content: space-between; position: relative; flex-grow: 1; -unity-text-align: middle-right; color: rgb(255, 255, 255); font-size: 26px;" />
                    <ui:DropdownField label="State" name="StateField" style="justify-content: space-around; flex-grow: 1; margin-right: 72px; -unity-text-align: middle-center; font-size: 26px; color: rgb(255, 255, 255);" />
                    <ui:Button text="ADD" parse-escape-sequences="true" display-tooltip-when-elided="true" name="AddTransitionBtn" style="-unity-font-style: bold;" />
                </ui:VisualElement>
            </ui:VisualElement>
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
